{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "baseUrl":"./src",
    "paths": {
      "@component/*": ["component/*"],
      "@page/*": ["page/*"]
      
    }
  },
  "include": ["src"]
}
